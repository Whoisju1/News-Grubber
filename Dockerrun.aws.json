{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "api",
      "hostName": "api",
      "image": "whoisju1/news-grubber-api",
      "essential": false
    },
    {
      "name": "client",
      "hostName": "client",
      "image": "whoisju1/news-grubber-client",
      "essential": true,
      "portMappings": [
       {
        "hostPort": 80,
        "containerPort": 80
       }
      ]
    }
  ],
  "links": ["api", "client"]
}