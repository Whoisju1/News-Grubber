{
  "AWSEBDockerrunVersion": 2,
  "ContainerDefinitions": [
    {
      "Name": "api",
      "HostName": "api",
      "Image": "whoisju1/news-grubber-api",
      "Essential": false
    },
    {
      "Name": "client",
      "HostName": "client",
      "Image": "whoisju1/news-grubber-client",
      "Essential": true,
      "PortMappings": [
       {
        "HostPort": 80,
        "ContainerPort": 80
       }
      ]
    }
  ],
  "links": ["api", "client"]
}